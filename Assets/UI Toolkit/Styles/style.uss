    font-size: 50px;
    color: rgb(255, 255, 255);
    text-shadow: 5px 5px 0 rgba(0, 0, 0, 0.29);
    letter-spacing: 5px;
    word-spacing: 0;
    -unity-text-align: middle-center;
}

.label {
    -unity-font-definition: url('project://database/Assets/Third%20Party%20Assets/GUI%20PRO%20Kit%20-%20Casual%20Game/ResourcesData/Fonts/LilitaOne-Regular%20SDF%201.asset?fileID=11400000&guid=3698d87c2a78fe14cbe7ada27ffd1b2a&type=2#LilitaOne-Regular SDF 1');
    color: rgb(255, 255, 255);
    -unity-text-align: middle-center;
    font-size: 26px;
    text-shadow: 5px 5px 0 rgba(0, 0, 0, 0.39);
    letter-spacing: 5px;
    -unity-font: url('project://database/Assets/Third%20Party%20Assets/GUI%20PRO%20Kit%20-%20Casual%20Game/ResourcesData/Fonts/LilitaOne-Regular.ttf?fileID=12800000&guid=c16eb7d7c3953489db00312b8b09df4b&type=3#LilitaOne-Regular');
    align-self: center;
    align-items: center;
}

.notification-icon {
    flex-grow: 1;
    background-color: rgba(0, 0, 0, 0);
    background-image: url('project://database/Assets/Images/Icons/Exclamation-Mark.png?fileID=2800000&guid=f3498b31d90814348870540b0c6f191c&type=3#Exclamation-Mark');
    rotate: 10deg;
}

.notification {
    flex-grow: 1;
    background-color: var(--yellow);
    width: 50px;
    height: 50px;
    position: absolute;
    border-top-left-radius: 50%;
    border-bottom-left-radius: 50%;
    border-top-right-radius: 50%;
    border-bottom-right-radius: 50%;
    border-left-width: var(--border);
    border-right-width: var(--border);
    border-top-width: var(--border);
    border-bottom-width: var(--border);
    border-left-color: var(--white);
    border-right-color: var(--white);
    border-top-color: var(--white);
    border-bottom-color: var(--white);
    top: -20px;
    right: -20px;
}

.play-button {
    height: 140px;
    font-size: 70px;
}

.preview-map {
    flex-grow: 0;
    background-color: rgb(34, 33, 38);
    flex-direction: column;
    width: 100%;
    height: 300px;
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
    border-left-width: var(--border);
    border-right-width: var(--border);
    border-top-width: var(--border);
    border-bottom-width: var(--border);
    border-top-left-radius: var(--radius);
    border-bottom-left-radius: var(--radius);
    border-top-right-radius: var(--radius);
    border-bottom-right-radius: var(--radius);
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 20px;
}

.settings-panel {
    border-left-color: var(--white);
    border-right-color: var(--white);
    border-top-color: var(--white);
    border-bottom-color: var(--white);
    border-left-width: var(--border);
    border-right-width: var(--border);
    border-top-width: var(--border);
    border-bottom-width: var(--border);
    border-top-left-radius: var(--radius);
    border-bottom-left-radius: var(--radius);
    border-top-right-radius: var(--radius);
    border-bottom-right-radius: var(--radius);
    background-color: var(--blue-1);
    width: 70%;
    margin-left: 50px;
    margin-right: 50px;
    margin-top: 50px;
    margin-bottom: 50px;
    padding-left: 25px;
    padding-right: 25px;
    padding-top: 25px;
    padding-bottom: 25px;
}

#unity-low-button {
    display: none;
}

#unity-high-button {
    display: none;
}

#unity-slider {
}

#unity-tracker {
    background-color: rgb(34, 33, 38);
    border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
