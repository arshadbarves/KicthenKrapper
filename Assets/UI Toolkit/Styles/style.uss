:root {
    --blue-1: rgb(51, 201, 234);
    --blue-2: rgb(2, 158, 247);
    --pink-1: rgb(243, 27, 247);
    --pink-2: rgb(240, 0, 144);
    --yellow: rgb(254, 185, 56);
    --green: rgb(102, 247, 15);
    --purple: rgb(70, 68, 175);
    --white: rgb(255, 255, 255);
    --dark: rgb(34, 33, 38);
    --radius: 25px;
    --border: 5px;
    --transition-duration: 0.3s;
    --transition-timing-function: ease-in-out;
}

.cotainer {
    flex-grow: 1;
    width: 100%;
    height: 100%;
}

.icon {
    flex-grow: 1;
    background-color: rgba(0, 0, 0, 0);
    background-image: url('project://database/Assets/Third%20Party%20Assets/GUI%20PRO%20Kit%20-%20Simple%20Casual/Sprite/Demo/Demo_Icon/Icon_WhiteIcon_Home.png?fileID=2800000&guid=5b1d610f5877042a581c418847709013&type=3#Icon_WhiteIcon_Home');
    width: 62px;
    height: 62px;
    -unity-background-scale-mode: scale-and-crop;
}

.background {
    flex-grow: 1;
    position: absolute;
    height: 100%;
    width: 100%;
    background-image: url('project://database/Assets/Images/bg-gd-overlay.png?fileID=2800000&guid=ada7c856e509d1342bc39192b37c404e&type=3#bg-gd-overlay');
}

.background-overlay {
    flex-grow: 1;
    background-image: url('project://database/Assets/Third%20Party%20Assets/GUI%20PRO%20Kit%20-%20Casual%20Game/ResourcesData/Sprite/Component/Slider/Slider19_Fusion_BgGlow.png?fileID=2800000&guid=1e6ae96ee68d643fbaba3efabe195a41&type=3#Slider19_Fusion_BgGlow');
    height: 100%;
    width: 100%;
    rotate: 0;
}

.button {
    background-color: var(--yellow);
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
    border-left-width: var(--border);
    border-right-width: var(--border);
    border-top-width: var(--border);
    border-bottom-width: var(--border);
    border-top-left-radius: var(--radius);
    border-bottom-left-radius: var(--radius);
    border-top-right-radius: var(--radius);
    border-bottom-right-radius: var(--radius);
    height: 125px;
    -unity-font: url('project://database/Assets/Third%20Party%20Assets/GUI%20PRO%20Kit%20-%20Casual%20Game/ResourcesData/Fonts/LilitaOne-Regular.ttf?fileID=12800000&guid=c16eb7d7c3953489db00312b8b09df4b&type=3#LilitaOne-Regular');
