    padding-top: 25px;
    padding-bottom: 25px;
}

.settings-option {
    flex-grow: 1;
    background-color: rgba(0, 0, 0, 0);
    margin-left: 25px;
    margin-right: 25px;
    margin-top: 25px;
    margin-bottom: 25px;
}

.divider {
    flex-grow: 1;
